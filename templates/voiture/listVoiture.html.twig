<h1>List des Voiture</h1>
<table border="1">
    <tr>
        <td>Serie</td>
        <td>Date M C</td>
        <td>Modele</td>
    </tr>
    {% for v in listVoiture %}
        <tr>
            <td>{{ v.Serie }}</td>
            <td>{{ v.dateMiseEnMarche | date }}</td>
            <td>{{ v.modele }}</td>
            <td>{{ v.prixJour }}</td>
            <td><a href="{{ path("voitureDelete", {'id': v.id}) }}">Delete</a></td>
            <td><a href="{{ path("voitureUpdate", {'id': v.id}) }}">Update</a></td>
        </tr>
    {% endfor %}
</table>
